                "old_password"
            ],
            "properties": {
                "new_password": {
                    "type": "string",
                    "minLength": 8,
                    "example": "example321"
                },
                "old_password": {
                    "type": "string",
                    "minLength": 8,
                    "example": "example123"
                }
            }
